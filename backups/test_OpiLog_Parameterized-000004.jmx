        <com.aevi.jmeter.wnsp.samplers.WNSPOpiLogMessageSampler guiclass="com.aevi.jmeter.wnsp.samplers.WNSPOpiLogGui" testclass="com.aevi.jmeter.wnsp.samplers.WNSPOpiLogMessageSampler" testname="WNSP_OpiLogMessage" enabled="true">
